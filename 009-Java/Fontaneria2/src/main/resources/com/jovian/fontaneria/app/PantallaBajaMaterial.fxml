      <Button fx:id="btnNuevaBusqueda" layoutX="429.0" layoutY="681.0" mnemonicParsing="false" onAction="#nuevaBusqueda" styleClass="btnMenu" stylesheets="@estilos.css" text="Nueva Busqueda" />
      <Button fx:id="btnBuscarNombre" layoutX="607.0" layoutY="681.0" mnemonicParsing="false" onAction="#buscarNombre" styleClass="btnMenu" stylesheets="@estilos.css" text="Buscar por Nombre" />
