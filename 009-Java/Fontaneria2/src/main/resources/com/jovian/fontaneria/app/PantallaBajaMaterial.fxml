<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" styleClass="AnchorPane" stylesheets="@estilos.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jovian.fontaneria.app.ControladorBajaMaterial">
   <children>
      <ImageView fitHeight="111.0" fitWidth="301.0" layoutX="74.0" layoutY="57.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/logo.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="111.0" fitWidth="76.0" layoutX="741.0" layoutY="75.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/bajaMaterial.png" />
         </image>
      </ImageView>
      <Label layoutX="854.0" layoutY="97.0" styleClass="lblAltaClientesHead" stylesheets="@estilos.css" text="BAJA MATERIAL" />
      <AnchorPane layoutX="60.0" layoutY="168.0" prefHeight="480.0" prefWidth="1080.0" styleClass="AnchorPaneDatos" stylesheets="@estilos.css">
